// Todos

model Todo {
  id        Int      @id @default(autoincrement())
  title     String
  status    String
  completed Boolean  @default(false)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model User {
  id    Int    @id @unique @default(autoincrement())
  name  String
  email String @unique
}